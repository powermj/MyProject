package com.zhxg.yqzj.service.exception.casebase;

public class CasebaseDownloadTimesRunOutException extends CasebaseException {

	/**
	* 
	*/
	private static final long serialVersionUID = 5642386106955836464L;

	/**
	 * 下载次数已耗尽
	 */
	protected static final String ERROR_CODE = "002";

	public CasebaseDownloadTimesRunOutException() {
		super(ERROR_CODE, "The casebase download times have run out.");
	}

	public CasebaseDownloadTimesRunOutException(String msg) {
		super(ERROR_CODE, msg);
	}
}

package com.zhxg.yqzj.service.exception.subject;

public class KeywordExceedingLimitException extends SubjectException {
    
    /**
     * 
     */
    private static final long serialVersionUID = 1L;
    
    protected static final String ERROR_CODE = "002";

    public KeywordExceedingLimitException() {
        super(ERROR_CODE, "关键词数量超限!");
    }

    public KeywordExceedingLimitException(String msg) {
        super(ERROR_CODE, msg);
    }
}

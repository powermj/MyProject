public class MobileCloudCourseLectuer extends BaseEntity {

    /**
     * 讲师自增id
     */
    private Integer id;
    /**
     * 讲师名字
     */
    private String lectuerName;
    /**
     * 讲师简介
     */
    private String lectuerSummary;
    /**
     * 讲师图片地址
     */
    private String lectuerPicUrl;
    
    public Integer getId() {
        return id;
    }
    
    public void setId(Integer id) {
        this.id = id;
    }
    
    public String getLectuerName() {
        return lectuerName;
    }

    
    public void setLectuerName(String lectuerName) {
        this.lectuerName = lectuerName;
    }

    public String getLectuerSummary() {
        return lectuerSummary;
    }

    
    public void setLectuerSummary(String lectuerSummary) {
        this.lectuerSummary = lectuerSummary;
    }

    
    public String getLectuerPicUrl() {
        return lectuerPicUrl;
    }

    
    public void setLectuerPicUrl(String lectuerPicUrl) {
        this.lectuerPicUrl = lectuerPicUrl;
    }
}
